{
	"name": "mdw-bronze",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "usraw",
						"type": "DatasetReference"
					},
					"name": "usiatiraw"
				},
				{
					"dataset": {
						"referenceName": "caniatiraw",
						"type": "DatasetReference"
					},
					"name": "caniatiraw"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText8",
						"type": "DatasetReference"
					},
					"name": "cabronze"
				}
			],
			"transformations": [
				{
					"name": "removeColumns",
					"description": "Autogenerated by data preview actions"
				},
				{
					"name": "filternullrows"
				},
				{
					"name": "union1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> usiatiraw",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> caniatiraw",
				"union1 select(mapColumn(",
				"          each(match(name!='_col7_')),",
				"          each(match(name!='_col8_'))",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> removeColumns",
				"removeColumns filter(!isNull('Receiver Organisation')) ~> filternullrows",
				"usiatiraw, caniatiraw union(byName: true)~> union1",
				"filternullrows sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['uscaniatibronze'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> cabronze"
			]
		}
	}
}