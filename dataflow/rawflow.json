{
	"name": "rawflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "copyuscsv",
						"type": "DatasetReference"
					},
					"name": "DataverseUS"
				},
				{
					"dataset": {
						"referenceName": "copycancsv",
						"type": "DatasetReference"
					},
					"name": "DataverseCan"
				},
				{
					"dataset": {
						"referenceName": "copygbcsv",
						"type": "DatasetReference"
					},
					"name": "DataverseGB"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "gbraw",
						"type": "DatasetReference"
					},
					"name": "DatalakeUS"
				},
				{
					"dataset": {
						"referenceName": "DelimitedText6",
						"type": "DatasetReference"
					},
					"name": "DatalakeCan"
				},
				{
					"dataset": {
						"referenceName": "copygbcsv",
						"type": "DatasetReference"
					},
					"name": "DatalakeGB"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DataverseUS",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DataverseCan",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DataverseGB",
				"DataverseUS sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['usiatiraw'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> DatalakeUS",
				"DataverseCan sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['caiatiraw'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> DatalakeCan",
				"DataverseGB sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['gbiatiraw'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> DatalakeGB"
			]
		}
	}
}