{
	"name": "ReturnRate",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ReturnRate",
						"type": "DatasetReference"
					},
					"name": "exampleRR"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText4",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filterreturned"
				},
				{
					"name": "ModifyColumns1",
					"description": "Autogenerated by data preview actions"
				},
				{
					"name": "MapDrifted1",
					"description": "Creates an explicit mapping for each drifted column"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> exampleRR",
				"MapDrifted1 filter(ReturnQty == 1) ~> filterreturned",
				"exampleRR derive(each(match(name == 'EVENT'), $$ = toString($$)),",
				"          each(match(name == 'Return Qty'), $$ = toInteger($$))) ~> ModifyColumns1",
				"ModifyColumns1 derive(Enterprise = toString(byName('Enterprise')),",
				"          {ROLLUP NAME} = toString(byName('ROLLUP NAME')),",
				"          {CUSTOMER ACCOUNT NUMBER} = toString(byName('CUSTOMER ACCOUNT NUMBER')),",
				"          {PRODUCT ISBT CODE} = toString(byName('PRODUCT ISBT CODE')),",
				"          ABORH = toString(byName('ABORH')),",
				"          EVENT = toString(byName('EVENT')),",
				"          ShipQty = toInteger(byName('Ship Qty')),",
				"          ReturnQty = toInteger(byName('Return Qty'))) ~> MapDrifted1",
				"filterreturned sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}